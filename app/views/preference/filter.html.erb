<h4>These are schools recommended based on your preferences</h4>


<% if @region_size_rank_schools.present? %>
  <p>
   <% @region_size_rank_schools.each do |school| %>
     <a href="/schools/<%= school.id %>"><p><%= school.name %></p></a>
   <% end %>
  </p>
<% else %>
  <h2><%= @error %></h2>
<% end %>
